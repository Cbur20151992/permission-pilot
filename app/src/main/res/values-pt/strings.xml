    <string name="permission_nfc_label">NFC</string>
    <string name="permission_nfc_description">Permite que o app se comunique com Near Field Communication (NFC) tags, cartões e leitores.</string>
    <string name="permission_write_media_storage_label">Gerenciamento da mídia</string>
    <string name="permission_write_media_storage_description">Permite que os apps modifiquem ou apaguem arquivos de mídia criados por outros apps SEM avisar você. Apps devem ter permissão de acesso a arquivos e mídias.</string>
    <string name="permission_package_usage_stats_label">Acesso aos dados de uso</string>
    <string name="permission_package_usage_stats_description">Permite que o app veja as estatísticas de uso de todos os apps instalados no seu dispositivo.</string>
    <string name="permission_write_settings_label">Modificação de configurações do sistema</string>
    <string name="permission_write_settings_description">Permite que o app modifique os dados de configuração do sistema. Apps maliciosos podem corromper a configuração do seu sistema.</string>
    <string name="permission_bluetooth_advertise_label">Anunciar o Bluetooth</string>
    <string name="permission_bluetooth_advertise_description">Permite que o app anuncie presença para outros dispositivos com Bluetooth por perto.</string>
    <string name="permission_get_accounts_label">Obter contas</string>
    <string name="permission_get_accounts_description">Permite que o app obtenha a lista de contas existentes no telefone. Isto pode incluir quaisquer contas criadas por apps que você tenha instalado.</string>
    <string name="permission_access_background_location_label">Acesso a localização em segundo plano</string>
    <string name="permission_access_background_location_description">Permite o acesso à localização em qualquer momento, mesmo enquanto o app não estiver em uso.</string>
    <string name="permission_access_media_location_label">Acesso a localização da mídia</string>
    <string name="permission_access_media_location_description">Permite que o app determine a localização da sua coleção de mídia.</string>
    <string name="permission_manage_accounts_label">Gerenciamento das contas</string>
    <string name="permission_manage_accounts_description">Permite que o app gerencie as contas no dispositivo.</string>
    <string name="permission_change_wifi_state_label">Alteração de estado do WiFi</string>
    <string name="permission_change_wifi_state_description">Permite que os apps liguem ou desliguem WiFi, examinem e conectem a redes WiFi, adicionem ou apaguem redes ou iniciem um HotSpot local.</string>
    <string name="permission_change_network_state_label">Alteração de estado da rede</string>
    <string name="permission_change_network_state_description">Permite que o app mude o estado de conectividade da rede.</string>
    <string name="permission_read_sync_settings_label">Leitura de configurações da sincronização</string>
    <string name="permission_read_sync_settings_description">Permite que o app leia as configurações de sincronização de uma conta.</string>
    <string name="permission_request_install_packages_label">Instalar apps desconhecidos (Fontes desconhecidas)</string>
    <string name="permission_request_install_packages_description">Allows apps to install other apps on your device. This may put your device and data at risk.</string>
    <string name="permission_manage_external_storage_label">All Files access</string>
    <string name="permission_manage_external_storage_description">Allows apps to read, modify and delete all files on your device as well as any connected storage devices. Apps with this permission may also access files that aren\'t related to the app, WITHOUT notifying you.</string>
    <string name="permission_system_alert_window_label">Appear on Top</string>
    <string name="permission_system_alert_window_description">Allows apps to show things on top of other apps you\'re using (including showing ads). This may interfere with your use of other apps.</string>
    <string name="permission_group_calendar_label">Calendário</string>
